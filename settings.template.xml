<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>/Users/sameer.khan/.m2/repository</localRepository>
  <interactiveMode>true</interactiveMode>
  <usePluginRegistry>false</usePluginRegistry>
  <offline>false</offline>
  <servers>
    <server>
      <id>artifactory.imanage.com</id>
      <username>%ARTIFACTORY_USERNAME%</username>
      <password>%ARTIFACTORY_PASSPHRASE%</password>
    </server>
    <server>
      <id>docker.imanage.com</id>
      <username>%ARTIFACTORY_USERNAME%</username>
      <password>%ARTIFACTORY_PASSPHRASE%</password>
    </server>
    <server>
      <id>artifactory.imanage.com-snapshots</id>
      <username>%ARTIFACTORY_USERNAME%</username>
      <password>%ARTIFACTORY_PASSPHRASE%</password>
    </server>
    <server>
      <id>artifactory.imanage.com-plugins-releases</id>
      <username>%ARTIFACTORY_USERNAME%</username>
      <password>%ARTIFACTORY_PASSPHRASE%</password>
    </server>
    <server>
      <id>artifactory.imanage.com-plugins-snapshots</id>
      <username>%ARTIFACTORY_USERNAME%</username>
      <password>%ARTIFACTORY_PASSPHRASE%</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <mirrorOf>*</mirrorOf>
      <name>remote-repos</name>
      <url>https://artifactory.imanage.com/artifactory/repo</url>
      <id>artifactory.imanage.com</id>
    </mirror>
  </mirrors>
  <activeProfiles>
    <activeProfile>ravn</activeProfile>
    <!-- <activeProfile>test</activeProfile> -->
  </activeProfiles>
  <profiles>
    <profile>
      <id>build</id>
      <repositories>
        <repository>
          <id>internal</id>
          <name>Archiva Managed Internal Repository</name>
          <url>http://archiva.imanage.com/repository/internal/</url>
          <releases>
              <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
            <updatePolicy>always</updatePolicy>
          </snapshots>
        </repository>
      </repositories>
    </profile>
    <profile>
      <id>ravn</id>
      <repositories>
        <repository>
          <releases>
              <enabled>true</enabled>
          </releases>
          <snapshots>
              <enabled>true</enabled>
          </snapshots>
          <id>repo.maven.apache.org</id>
          <name>Maven Central Repository</name>
          <url>https://repo.maven.apache.org/maven2/</url>
        </repository>
        <repository>
            <id>GroupDocsJavaAPI</id>
            <name>GroupDocs Java API</name>
            <url>https://artifact.groupdocs.com/repo/</url>
        </repository>
        <repository>
          <id>artifactory.imanage.com</id>
          <name>libs-releases</name>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
            <updatePolicy>never</updatePolicy>
            <checksumPolicy>fail</checksumPolicy>
          </snapshots>
          <url>https://artifactory.imanage.com/artifactory/libs-releases</url>
        </repository>
        <repository>
          <id>artifactory.imanage.com-snapshots</id>
          <name>libs-snapshots</name>
          <releases>
            <enabled>false</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>never</updatePolicy>
            <checksumPolicy>fail</checksumPolicy>
          </snapshots>
          <url>https://artifactory.imanage.com/artifactory/libs-snapshots</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>artifactory.imanage.com-plugins-releases</id>
          <name>plugins-releases</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>https://artifactory.imanage.com/artifactory/plugins-releases</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots/>
          <id>artifactory.imanage.com-plugins-snapshots</id>
          <name>plugins-snapshots</name>
          <url>https://artifactory.imanage.com/artifactory/plugins-snapshots</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>